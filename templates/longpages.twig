{% extends "_layout.twig" %}

{% set title = "Long pages" %}
{% set noindex = true %}
{% set pagename = "/Special:LongPages" %}

	{% block content %}
<h1>Long pages</h1>

<p>Showing the 50 longest pages.</p>

<ol>
{% for page in longpages %}
	<li><a href="/{{ page.title | replace({" ":"_"}) }}">{{ page.title }}</a> [{{ page.size | number_format(0, '', ' ') }} bytes]</li>
{% endfor %}
</ol>

	{% endblock %}